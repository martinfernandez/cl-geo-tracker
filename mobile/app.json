{
  "expo": {
    "name": "GPS Tracker",
    "slug": "gps-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gpstracker.app"
    },
    "android": {
      "package": "com.gpstracker.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow GPS Tracker to access your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GPS Tracker to access your photos.",
          "cameraPermission": "Allow GPS Tracker to use your camera."
        }
      ]
    ]
  }
}
